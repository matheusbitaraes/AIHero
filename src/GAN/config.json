{
  "generated_evidences_dir": "data/evidences/gifs",
  "verbose": true,
  "checkpoint": {
    "use_checkpoint": true,
    "checkpoint_folder": "data/training_checkpoints"
  },
  "training": {
    "num_examples_to_generate": 4,
    "num_epochs": 400,
    "train_data_folder": "data/train",
    "noise_dim": 100,
    "batch_percentage": 0.2,
    "buffer_percentage": 1
  },
  "data_augmentation": {
    "enabled": true,
    "replication_factor": 50,
    "data_augmentation_strategy_pipeline": [
      {"method":"TimeChangeStrategy", "factor": 1},
      {"method":"NoteJoinStrategy", "factor": 1},
      {"method":"OctaveChangeStrategy", "factor": 1}
    ]
  }
}
